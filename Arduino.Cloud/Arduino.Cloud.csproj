<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Arduino Cloud API</Title>
    <AssemblyTitle>Arduino Cloud API</AssemblyTitle>
    <Company>Null Software</Company>
    <Description>Arduino Cloud API for IoT. 
Provides ability to interact with dashboards, devices, properties etc.</Description>
    <Copyright>Copyright © Null Software 2022. All rights reserved.</Copyright>
    <PackageTags>Arduino IoT Cloud</PackageTags>
    <!--<GenerateDocumentationFile>true</GenerateDocumentationFile>-->
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>arduino.png</PackageIcon>
    <PackageVersion>0.0.1.15-alpha</PackageVersion>
    <AssemblyVersion>0.0.1.15</AssemblyVersion>
    <FileVersion>0.0.1.15</FileVersion>
    <PackageReleaseNotes>Added icon.</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="arduino.png" Pack="true" PackagePath=""/>
  </ItemGroup>

</Project>
